                            @using (Html.BeginForm("EsquemaVacunacion", "Vacuna", FormMethod.Post, new { enctype = "multipart/form-data" }))
                            {
                                @Html.AntiForgeryToken()
                                @Html.ValidationSummary(true)
                                @Html.HiddenFor(model => model.hojaVidaId)
                                @Html.HiddenFor(model => model.vacunaId)
                                <div class="col-sm-9 col-md-9">

                                    
                                    <fosrm class="form-horizontal" role="form">
                                        <legend>Datos vacuna</legend>
                                        <div class="col-sm-9 col-md-9">

                                            <div class="form-group">
                                                <label class="col-sm-2 control-label">Aplicada</label>

                                                <div class="col-sm-4">
                                                    @Html.TextBoxFor(model => model.fecha_vacunacion, new { @class = "form-control", @readonly = "readonly" })
                                                    @Html.ValidationMessageFor(model => model.fecha_vacunacion)
                                                </div>

                                                <label class="col-sm-2 control-label">Próxima vacuna</label>
                                                <div class="col-sm-4">
                                                    @Html.TextBoxFor(model => model.fecha_prox_vacunacion, new { @class = "form-control", @readonly = "readonly" })
                                                    @Html.ValidationMessageFor(model => model.fecha_prox_vacunacion)
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="col-sm-2 control-label">Vacuna</label>

                                                <div class="col-sm-4">
                                                    @Html.TextBoxFor(model => model.nombre_generico, new { @class = "form-control", @readonly = "readonly" })
                                                    @Html.ValidationMessageFor(model => model.nombre_generico)
                                                </div>
                                                <label class="col-sm-2 control-label">Lote</label>
                                                <div class="col-sm-4">
                                                    @Html.TextBoxFor(model => model.lote, new { @class = "form-control" })
                                                    @Html.ValidationMessageFor(model => model.lote)

                                                </div>

                                            </div>
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label">Laboratorio</label>

                                                <div class="col-sm-4">
                                                    @Html.DropDownList("laboratorio_vacunacion", new List<SelectListItem>() { new SelectListItem { Text = "BIOTOSCANA", Value = "BIOTOSCANA" }, new SelectListItem { Text = "SANOFI", Value = "SANOFI" }, new SelectListItem { Text = "DELTA", Value = "DELTA" }, new SelectListItem { Text = "GLAXO", Value = "GLAXO" }, new SelectListItem { Text = "MERK", Value = "MERK" }, new SelectListItem { Text = "OTRO", Value = "OTRO" } }, new { @class = "form-control" })
